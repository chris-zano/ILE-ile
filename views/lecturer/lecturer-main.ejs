<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/favicon" type="image/x-icon">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" />
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="/css/global/root">
    <link rel="stylesheet" href="/css/global/header">
    <link rel="stylesheet" href="/css/global/sidenav">
    <link rel="stylesheet" href="/css/global/main">
    <% for( let styleUrl of stylesheets) { %>
        <link rel="stylesheet" href="<%= styleUrl %>">
    <% } %>
    <title>
        ITLE(Lecturer): <%= pageTitle %>
    </title>
</head>

<body>
    
    <!-- include header -->
    <%- include('templates/header') %>

    <nav class="sidebar">
        <div class="top">
            <div class="logo">
                <i>
                    <div>icons by <a href="https://icons8.com">Icons8</a></div>
                </i>
                <span></span>
            </div>
            <i class="" id="btn">
                <img
                    src="data:image/png;base64,">
            </i>
        </div>
        <div class="user">
            <div>
                <section class="adminProfileCard">
                    <div>
                        <img src="/images/darkSide" alt="me" class="user-img">
                        <div class="img-div">
                            <p class="bold">
                                <%= lecturer.firstname %>
                                    <%= lecturer.lastname %>
                            </p>
                            <p class="role">
                                <%= lecturer.faculty%>
                            </p>
                        </div>
                    </div>
                </section>
            </div>
        </div>
        <ul>
            <li>
                <a href="/lecturers/render/dashboards/<%= lecturer.id %>">
                    <i class="">
                        <img src="/images/dashboard" alt="dashboard">
                    </i>
                    <span class="nav-item">Dashboard</span>
                </a>
                <span class="tooltip">Dashboard</span>
            </li>
            <li>
                <a href="/lecturers/render/courses/<%= lecturer.id %>">
                    <i class=''>
                        <img src="/images/courses" alt="courses">
                    </i>
                    <span class="nav-item">courses</span>
                </a>
                <span class="tooltip">courses</span>
            </li>
            <li>
                <a href="/lecturers/render/schedules/<%= lecturer.id %>">
                    <i class="">
                        <img src="/images/schedule" alt="schedule">
                    </i>
                    <span class="nav-item">schedules</span>
                </a>
                <span class="tooltip">schedules</span>
            </li>
            <li>
                <a href="/lecturers/render/assignments/<%= lecturer.id %>">
                    <i class="">
                        <img src="/images/assignment" alt="assignment">
                    </i>
                    <span class="nav-item">assignments</span>
                </a>
                <span class="tooltip">assignments</span>
            </li>
            <li>
                <a href="/lecturers/render/live lectures/<%= lecturer.id %>">
                    <i class="">
                        <img src="/images/live" alt="go live">
                    </i>
                    <span class="nav-item">Go Live</span>
                </a>
                <span class="tooltip">Go Live</span>
            </li>
        </ul>
        <div class="logout-navsec">
            <li>
                <a href="#">
                    <i class="">
                        <img src="/images/logout" alt="logout">
                    </i>
                    <span class="nav-item">Logout</span>
                </a>
                <span class="tooltip">Logout</span>
            </li>
        </div>
    </nav>
    <script>
        let btn = document.querySelector('#btn');
        let sidebar = document.querySelector('.sidebar');

        btn.onclick = () => {
            sidebar.classList.toggle('active');
        }
    </script>
    <main class="wrapper-main main-content" id="wrapper-main">
        
    </main>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/script/utils/admin/util.restful"></script>
    <script src="/script/scripts/admin/global.search"></script>
    <% for( let scriptUrl of scripts) { %>
        <script src="<%= scriptUrl %>"></script>
    <% } %>
</body>

</html>