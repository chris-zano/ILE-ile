<div class="section-header">
    <div class="table-buttons-nav">
        <button type="button" id="nextPage">Next</button>
        <button type="button" id="resetBtn">Reset</button>
    </div>

    <div class="section-header-nav">
        <a href="/admin/users/createOne?id=<%= _id %>&&adminId=<%= adminId %>&&role=<%= role %>">Create new
            User</a>
        <a href="/admin/users/import/students?id=<%= _id %>&&adminId=<%= adminId %>&&role=<%= role %>">Import
            Users</a>
        <a href="/admin/users/manage?id=<%= _id %>&&adminId=<%= adminId %>&&role=<%= role %>">Manage
            Users</a>
    </div>
</div>

<!-- list of available users -->

<div class="section-users section-body">
    <div class="section-body-header">
        <br>
        <div class="students" data-offset="0">
            <table>
                <thead>
                    <th class="table-head" id="count">No</th>
                    <th class="table-head" id="tstudentId">StudentId</th>
                    <th class="table-head" id="tfirstname">Firstname</th>
                    <th class="table-head" id="tlastname">Lastname</th>
                    <th class="table-head" id="tlevel">
                        <select name="level" id="studentLevel" class="dropdown">
                            <option value="none">Level</option>
                            <option value="100">100</option>
                            <option value="200">200</option>
                            <option value="300">300</option>
                            <option value="400">400</option>
                            <option value="Diploma">Diploma</option>
                            <option value="Top-up">Top-up</option>
                        </select>
                    </th>
                    <th class="table-head" id="tprogram">
                        <select name="program" id="program" class="dropdown">
                            <option value="none">Program</option>
                            <option value="BSc. Computer Engineering">BSc. Computer Engineering</option>
                            <option value="BSc. Telecom Engineering">BSc. Telecom Engineering</option>
                            <option value="Dip. Electrical Engineering">Dip. Electrical Engineering</option>
                        </select>
                    </th>
                    <th class="table-head" id="department">
                        <select name="department" id="department" class="dropdown">
                            <option value="none">Department</option>
                            <option value="Computer Engineering">Computer Engineering</option>
                            <option value="Telecom Engineering">Telecom Engineering</option>
                            <option value="Information Technology">Information Technology</option>
                        </select>
                    </th>
                    <th class="table-head" id="tcourses">Courses</th>
                    <th class="table-head" id="trepos">Repositories</th>
                    <th class="table-head" id="tfiles">Submissions</th>
                </thead>
                <tbody id="students-list">

                </tbody>
            </table>
        </div>
        <script src="/script/scripts/admin/forge"></script>
    </div>
</div>